{
    "name": "xlfederalelk0lx/claro-musica-api",
    "description": "Script para utilizar la Api de claro musica y poder obtener listas de reproduccion, generos y canciones.",
    "type": "Project",
    "require": {
        "php-curl-class/php-curl-class": "^8.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "authors": [
        {
            "name": "Jose Lui Coyotzi Ipatzi",
            "email": "coil811122@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
